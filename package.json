{
  "name": "npm-keep-me-logged-in",
  "version": "1.1.3",
  "description": "Keeps you logged in to npm registry so you don't have to run `npm login` every day",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "npm-keep-me-logged-in": "./bin/cli.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "start": "node dist/index.js"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "npm",
    "auth",
    "login",
    "token",
    "cli"
  ],
  "author": "Viljami Kuosmanen <viljami@viljami.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anttiviljami/npm-keep-me-logged-in.git"
  },
  "bugs": {
    "url": "https://github.com/anttiviljami/npm-keep-me-logged-in/issues"
  },
  "homepage": "https://github.com/anttiviljami/npm-keep-me-logged-in#readme",
  "dependencies": {
    "@inquirer/prompts": "^5.0.0",
    "chalk": "^5.3.0",
    "ini": "^4.1.0",
    "js-yaml": "^4.1.0",
    "execa": "^8.0.1"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/node": "^20.0.0",
    "@types/ini": "^4.0.0",
    "@types/js-yaml": "^4.0.9"
  }
}
